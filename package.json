{
  "name": "simple-react-material-grid",
  "version": "0.0.0",
  "description": "Simple React Data Grid example",
  "main": "index.js",
  "scripts": {
    "start": "browserify --debug --extension=js -t browserify-css -o bundle.js index.js && http-server -p 8000",
    "copy": "browserify --debug --extension=js -t browserify-css -o bundle.js index.js && ./bin/copy.sh"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "css-loader": "^0.28.7",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-css": "^0.10.0",
    "http-server": "^0.9.0"
  }
}
